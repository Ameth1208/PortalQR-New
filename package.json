{
  "name": "@ameth1208/portal-qr",
  "version": "0.1.31",
  "description": "Portal WEB para escanear QR",
  "main": "./lib/portal.http.cjs",
  "types": "module",
  "files": [
    "./dist/*",
    "./lib/portal.http.cjs"
  ],
  "devDependencies": {
    "@builder.io/qwik": "0.16.2",
    "@builder.io/qwik-city": "0.0.128",
    "@types/eslint": "8.4.10",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "latest",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "eslint": "8.31.0",
    "eslint-plugin-qwik": "0.16.2",
    "node-fetch": "3.3.0",
    "prettier": "2.8.1",
    "typescript": "4.9.4",
    "undici": "5.14.0",
    "vite": "4.0.3",
    "vite-tsconfig-paths": "3.5.0",
    "@rollup/plugin-commonjs": "^23.0.7"
  },
  "dependencies": {
    "kleur": "^4.1.5",
    "polka": "^0.5.2",
    "serve-static": "^1.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ameth1208/PortalQR.git"
  },
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.full": "qwik build && rollup --config ./rollup-portal.config.js && md portalQR && copy package.json portalQR\\package.json && move dist portalQR\\ && move lib portalQR\\",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build.portal": "rollup --config ./rollup-portal.config.js",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
